#include <iostream>
#include <string>
#include <math.h>
using namespace std;

int main(){

    string str;
    getline(cin , str);

    cout << "Enter the string: ";
    cout << str <<endl;

    str.push_back('s');
    cout << " New String: ";
    cout << str<<endl;

    str.pop_back();
    cout <<"Modified string: ";
    cout << str <<endl;
}

